pipeline {
    agent any
    
    stages {
        stage('Clone git') {
            steps {
                git 'https://github.com/cmelgreen/PersonalSite'
            }
        }



        stage('Build') { 
            steps {
                sh 'npm run-script build'
            }
        }
    }
}

        stage('Copy packages') {
            steps {
                sh 'copy /usr/lib/node_modules /var/jenkins_home/workspace/npm/node_modules'
            }
        }